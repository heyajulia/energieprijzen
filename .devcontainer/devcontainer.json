{
  "build": {
    "dockerfile": "Dockerfile"
  },
  "containerUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "a-h.templ",
        "esbenp.prettier-vscode",
        "GitHub.vscode-github-actions",
        "golang.go",
        "ms-azuretools.vscode-docker",
        "sourcegraph.cody-ai"
      ],
      "settings": {
        "cody.telemetry.level": "off"
      }
    }
  },
  "postAttachCommand": ".devcontainer/post-attach.sh"
}
