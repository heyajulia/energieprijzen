{
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "denoland.vscode-deno",
        "dhall.dhall-lang",
        "dhall.vscode-dhall-lsp-server",
        "GitHub.copilot-labs",
        "GitHub.copilot",
        "ms-azuretools.vscode-docker"
      ],
      "settings": {
        "deno.enable": true,
        "editor.formatOnSave": true
      }
    }
  },
  "postAttachCommand": "deno cache src/index.ts"
}
