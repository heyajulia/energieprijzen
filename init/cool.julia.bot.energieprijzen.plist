<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>cool.julia.bot.energieprijzen</string>
    
    <key>ProgramArguments</key>
    <array>
      <string>/Users/julia/bin/energieprijzen</string>
    </array>
    
    <key>EnvironmentVariables</key>
    <dict>
      <key>ENERGIEPRIJZEN_BOT_TOKEN</key>
      <string>your_variable_value</string>
      <key>ENERGIEPRIJZEN_BOT_CHAT_ID</key>
      <string>your_variable_value</string>
    </dict>
    
    <key>StartCalendarInterval</key>
    <dict>
      <key>Hour</key>
      <integer>15</integer>
      <key>Minute</key>
      <integer>1</integer>
    </dict>
    
    <key>StandardOutPath</key>
    <string>/Users/julia/Library/Logs/energieprijzen/stdout.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/julia/Library/Logs/energieprijzen/stderr.log</string>
  </dict>
</plist>
